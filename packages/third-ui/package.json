{
  "name": "@windrun-huaiin/third-ui",
  "version": "11.0.6",
  "description": "Third-party integrated UI components for windrun-huaiin projects",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./clerk": {
      "types": "./dist/clerk/index.d.ts",
      "import": "./dist/clerk/index.mjs",
      "require": "./dist/clerk/index.js"
    },
    "./clerk/server": {
      "types": "./dist/clerk/server.d.ts",
      "import": "./dist/clerk/server.mjs",
      "require": "./dist/clerk/server.js"
    },
    "./fingerprint": {
      "types": "./dist/clerk/fingerprint/index.d.ts",
      "import": "./dist/clerk/fingerprint/index.mjs",
      "require": "./dist/clerk/fingerprint/index.js"
    },
    "./fingerprint/server": {
      "types": "./dist/clerk/fingerprint/server.d.ts",
      "import": "./dist/clerk/fingerprint/server.mjs",
      "require": "./dist/clerk/fingerprint/server.js"
    },
    "./main": {
      "types": "./dist/main/index.d.ts",
      "import": "./dist/main/index.mjs",
      "require": "./dist/main/index.js"
    },
    "./main/server": {
      "types": "./dist/main/server.d.ts",
      "import": "./dist/main/server.mjs",
      "require": "./dist/main/server.js"
    },
    "./fuma/server": {
      "types": "./dist/fuma/server.d.ts",
      "import": "./dist/fuma/server.mjs",
      "require": "./dist/fuma/server.js"
    },
    "./fuma/mdx": {
      "types": "./dist/fuma/mdx/index.d.ts",
      "import": "./dist/fuma/mdx/index.mjs",
      "require": "./dist/fuma/mdx/index.js"
    },
    "./fuma/base": {
      "types": "./dist/fuma/base/index.d.ts",
      "import": "./dist/fuma/base/index.mjs",
      "require": "./dist/fuma/base/index.js"
    },
    "./lib/server": {
      "types": "./dist/lib/server.d.ts",
      "import": "./dist/lib/server.mjs",
      "require": "./dist/lib/server.js"
    },
    "./styles/third-ui.css": "./src/styles/third-ui.css",
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "build:prod": "rollup -c rollup.config.mjs",
    "dev": "rollup -c rollup.config.mjs --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/localizations": "catalog:",
    "@clerk/nextjs": "catalog:",
    "@clerk/types": "catalog:",
    "@fingerprintjs/fingerprintjs": "catalog:",
    "@windrun-huaiin/base-ui": "workspace:^",
    "@windrun-huaiin/lib": "workspace:^",
    "class-variance-authority": "catalog:",
    "fumadocs-core": "catalog:",
    "fumadocs-mdx": "catalog:",
    "fumadocs-typescript": "catalog:",
    "fumadocs-ui": "catalog:",
    "katex": "catalog:",
    "mermaid": "catalog:",
    "react-medium-image-zoom": "catalog:",
    "swiper": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "clsx": "catalog:",
    "next": "catalog:",
    "next-intl": "catalog:",
    "next-themes": "catalog:",
    "nprogress": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tailwind-merge": "catalog:",
    "tailwindcss": "catalog:"
  },
  "devDependencies": {
    "@types/mdx": "catalog:",
    "@types/node": "catalog:",
    "@types/nprogress": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:"
  },
  "keywords": [
    "ui",
    "components",
    "react",
    "nextjs",
    "clerk",
    "fumadocs",
    "tailwindcss"
  ],
  "author": "windrun-huaiin",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
