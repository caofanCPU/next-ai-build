{
  "name": "@windrun-huaiin/lib",
  "version": "12.1.0",
  "description": "Common utilities and configuration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js"
    },
    "./llm-utils": {
      "types": "./dist/llm-utils.d.ts",
      "import": "./dist/llm-utils.mjs",
      "require": "./dist/llm-utils.js"
    },
    "./common-app-config": {
      "types": "./dist/common-app-config.d.ts",
      "import": "./dist/common-app-config.mjs",
      "require": "./dist/common-app-config.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "build:prod": "rollup -c rollup.config.mjs",
    "dev": "rollup -c rollup.config.mjs --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src"
  },
  "dependencies": {
    "date-fns": "catalog:",
    "remark": "catalog:",
    "remark-frontmatter": "catalog:",
    "remark-gfm": "catalog:",
    "remark-mdx": "catalog:",
    "unist-util-visit": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "clsx": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tailwind-merge": "catalog:"
  },
  "keywords": [
    "utilities",
    "config",
    "typescript",
    "react",
    "nextjs"
  ],
  "author": "windrun-huaiin",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
